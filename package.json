{
    "name": "vitepress-example",
    "description": "A VitePress Demo Project",
    "author": "busyhe <busyhe@qq.com>",
    "version": "1.0.0",
    "main": "index.js",
    "scripts": {
        "docs:dev": "vitepress dev docs",
        "docs:build": "vitepress build docs",
        "docs:serve": "vitepress serve docs",
        "postinstall": "husky install",
