        "packages/*"
    ],
    "packageManager": "yarn@3.2.1",
    "devDependencies": {
        "@semantic-release/changelog": "^6.0.1",
        "@semantic-release/git": "^10.0.1",
        "commitizen": "^4.2.4",
        "cz-conventional-changelog": "^3.3.0",
        "husky": "^8.0.1",
        "lint-staged": "^13.0.3",
        "pinst": "^3.0.0",
        "semantic-release": "^19.0.2",
        "vitepress": "^1.0.0-alpha.4",
        "vue": "^3.2.37"
    },
    "homepage": "https://busyhe.github.io/vitepress-example/",
    "bugs": {
        "url": "https://github.com/busyhe/vitepress-example/issues"
    },
    "repository": {
        "type": "git",
        "url": "git@github.com:busyhe/vitepress-example.git"
    },
    "config": {
        "commitizen": {
            "path": "cz-conventional-changelog"
        }
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx,vue}": "eslint --fix"
    },
    "engines": {
        "node": ">=16.0.0"
    }
}
